{
  "name": "vue-byte-guide",
  "version": "0.1.0",
  "description": "A new feature guide component by Vue",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "types"
  ],
  "scripts": {
    "example:dev": "cd example && vite",
    "example:build": "cd example && vite build",
    "tsc": "tsc",
    "tsc:dev": "tsc -w",
    "copyAssets": "copyfiles -u 1 'src/**/*.less' lib",
    "build": "run-s tsc copyAssets",
    "bundle": "rollup -c rollup.config.js",
    "prepublishOnly": "run-p build bundle"
  },
  "dependencies": {
    "vue-types": "^4.1.1"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "copyfiles": "^2.4.1",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.70.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "vue": "^2.6 || ^3.2.0"
  }
}
